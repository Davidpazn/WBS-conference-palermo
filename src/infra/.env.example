# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5-nano
OPENAI_PROJECT_ID=your_project_id

# E2B Sandbox Configuration
E2B_API_KEY=your_e2b_api_key_here

# Letta Memory Configuration (optional)
USE_LETTA=false
LETTA_API_KEY=your_letta_api_key_here
LETTA_BASE_URL=http://localhost:8283

# EXA Search API Configuration (optional)
EXA_API_KEY=your_exa_api_key_here
EXA_BASE_URL=https://api.exa.ai

# Qdrant Vector Database Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_COLLECTION_NAME=agents_knowledge

# OpenTelemetry Configuration
OTEL_SERVICE_NAME=agents-backend
OTEL_SERVICE_VERSION=0.1.0
ENVIRONMENT=development

# LangSmith + OpenTelemetry Configuration
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=agents-demo
LANGSMITH_TRACING=true

# OTLP Configuration for LangSmith
OTEL_EXPORTER_OTLP_ENDPOINT=https://api.smith.langchain.com/otel
OTEL_EXPORTER_OTLP_HEADERS=x-api-key=your_langsmith_api_key_here,Langsmith-Project=agents-demo
OTEL_SEMCONV_STABILITY_OPT_IN=gen-ai

# Optional: Console exporter for development
OTEL_CONSOLE_EXPORTER=false

# HITL Configuration
CHECKPOINT_DB_PATH=sqlite:///checkpoints.db
USE_CODING_WORKFLOW=true
ENABLE_EXECUTION_REVIEW=false
SKIP_CLEAN_EXECUTION_REVIEW=true

# FastAPI Configuration
API_HOST=127.0.0.1
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Backend Version
BACKEND_VERSION=0.1.0